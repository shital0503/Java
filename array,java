import  java.io.*;
import  java.util.*;
class demo
{
    public static void main(String s[])
    {
        int a[]={10,5,6,20,30,55};
        System.out.println(" list of even no's form array");
        
        for(int i=0;i<=a.length;i++)
        {
            if(a[i]%2==0)
            {
                System.out.println("even no's"+a[i]);
            }
            else
            {
                System.out.println("odd no's"+a[i]);
            }
        }
    }
}
